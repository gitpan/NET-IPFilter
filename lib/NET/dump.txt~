if ( $IPtoCheck > $RangTo || $IPtoCheck < $RangFrom ) {
			
			# print "$IPtoCheck is bigger then rangto ";
			return 1;	# not to be blocked
	
		} elsif ( $IPtoCheck == $RangFrom || $IPtoCheck == $RangTo ) {
					
			return 0;	# to be blocked
			
		} elsif ( $IPtoCheck > $RangFrom && $IPtoCheck < $RangTo ) {
			
			return 0;	# to be blocked
				
		}; # if ( $IPtoCheck > $RangTo || $IPtoCheck < $RangFrom ) {